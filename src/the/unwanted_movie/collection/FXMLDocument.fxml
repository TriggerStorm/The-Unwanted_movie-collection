<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="649.0" prefWidth="756.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="the.unwanted_movie.collection.FXMLDocumentController">
    <children>
        <Button id="tc_edit" fx:id="button" layoutX="60.0" layoutY="75.0" onAction="#handleButtonAction" text="Edit" />
      <TableView layoutX="60.0" layoutY="100.0" prefHeight="449.0" prefWidth="637.0">
        <columns>
          <TableColumn id="tc_name" onEditCommit="#handleButtonAction" prefWidth="214.0" text="Name" />
          <TableColumn id="tc_cat" prefWidth="149.0" text="Category" />
            <TableColumn id="tc_rat" prefWidth="106.0" text="Rating" />
            <TableColumn id="tc_lastv" prefWidth="167.0" text="Last viewed" />
        </columns>
      </TableView>
      <TextField id="tf_search" layoutX="473.0" layoutY="75.0" />
    </children>
</AnchorPane>
